@model IEnumerable<Position>
<div class="card card-body">
    <div class="row g-0 py-3 border-bottom border-dashed border-300 align-items-end justify-content-between">
        <div class="col-auto">
            <h3 class="flex-1 mb-0 text-nowrap me-3">Help Wanted</h3>
        </div>
        <div class="col-auto"><a class="fw-bold fs--1" href="#!">See more</a></div>
    </div>
    @foreach (var item in Model)
    {
        @await Html.PartialAsync("Positions/_PositionListItem", item)
    }
</div>