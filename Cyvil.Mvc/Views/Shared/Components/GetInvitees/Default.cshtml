@model IEnumerable<SelectableUserModel>


@if(Model.Any())
{
    @foreach(var item in Model)
    {
        @if (!item.Selected)
        {
            <div class="form-check">
                <input class="form-check-input" type="checkbox" value="@item.Id" name="selectedUsers" id="user-@item.Id">
                <label class="form-check-label" for="user-@item.Id">
                    @item.Name
                </label>
            </div>
        }
        
    }
}
else
{
    <p>No users to invite</p>
}
