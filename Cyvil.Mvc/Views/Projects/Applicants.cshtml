@model IEnumerable<Applicant>

@{
    ViewData["Title"] = "Applicants";
    long projectId = ViewBag.ProjectId;
}

@section Stylesheets {
@* <link href="~/vendors/flatpickr/flatpickr.min.css" rel="stylesheet"> *@
}

<div class="content">
    @await Html.PartialAsync("Applicants/_ApplicantTable", Model)
    <partial name="_Footer" />
</div>

<vc:show-applicant></vc:show-applicant>

@section ProjectNav {
    @await Html.PartialAsync("Projects/_ProjectNav", projectId)
}

@section VendorScripts {
@* <script src="~/vendors/echarts/echarts.min.js"></script> *@
}

@section CustomScripts {
<script src="~/js/app.js"></script>
<script>
    showApplicant();
    selectApplicant();
</script>
}